{
  "manifest_version": 2,
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "{{ version }}",
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "background": {
    "scripts": [
      "barbblock.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}